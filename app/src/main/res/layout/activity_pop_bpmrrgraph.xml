<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.polito.cesarldm.tfg_bitadroidbeta.PopBPMRRGraphActivity"
    android:background="@color/colorPrimaryLight">

    <RelativeLayout
        android:id="@+id/rl_PU_graphBPM"
        android:layout_width="300dp"
        android:layout_height="150dp"
        android:layout_margin="5dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:background="@drawable/graph_item_shape">

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_PU_graphRR"
        android:layout_width="300dp"
        android:layout_height="150dp"
        android:layout_margin="5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rl_PU_graphBPM"
        app:layout_constraintVertical_bias="0.08"
        android:background="@drawable/graph_item_shape">

    </RelativeLayout>
</android.support.constraint.ConstraintLayout>
